{
  "name": "starhackit-ui",
  "title": "StarHack.It",
  "version": "10.14.0",
  "description": "React + node starter kit. A fullstack boilerplate with authentication and authorisation",
  "homepage": "https://github.com/FredericHeem/starhackit",
  "license": "NLPL",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js --open",
    "start:admin": "webpack serve --config webpack.dev.js --open-page=admin/users",
    "start:user": "webpack serve --config webpack.dev.js --open-page=user/profile",
    "start:public": "webpack serve --config webpack.dev.js --open-page=public",
    "start:prod": "http-server dist/",
    "prestart": "npm run prebuild",
    "lint": "tslint -c tslint.json './src/**/*.{js,jsx,ts,tsx}'",
    "cpd": "jscpd -r console --format 'javascript,typescript' ./src",
    "prebuild": "npm run lint && npm run cpd",
    "build": "webpack --config webpack.prod.js",
    "clean": "rimraf dist coverage",
    "test": "karma start --single-run --no-auto-watch karma.conf.js",
    "test:watch": "karma start --auto-watch --no-single-run karma.conf.js",
    "e2e": "npm run nightwatch",
    "nightwatch": "cd test/nightwatch/ && nightwatch",
    "docker:build": "docker build -t ui .",
    "docker:tag": "docker tag ui fredericheem/ui:latest",
    "docker:push": "docker push fredericheem/ui:latest",
    "docker:btp": "npm run docker:build && npm run docker:tag && npm run docker:push",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:FredericHeem/starhackit.git"
  },
  "bugs": {
    "url": "https://github.com/FredericHeem/starhackit/issues"
  },
  "browserslist": "last 2 versions, ie 11",
  "dependencies": {
    "@emotion/react": "11.1.4",
    "@emotion/styled": "11.0.0",
    "@material-ui/core": "4.11.2",
    "@types/react": "17.0.0",
    "axios": "0.21.1",
    "debug": "4.3.1",
    "history": "5.0.0",
    "i18next": "19.8.4",
    "i18next-browser-languagedetector": "6.0.1",
    "i18next-xhr-backend": "3.2.2",
    "intl": "1.2.5",
    "lodash": "4.17.20",
    "mdlean": "0.29.0",
    "mitt": "2.1.0",
    "mobx": "6.0.4",
    "mobx-react": "7.0.5",
    "prop-types": "15.7.2",
    "qs": "6.9.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "4.1.0",
    "react-leanimate": "0.16.0",
    "react-pagify": "2.4.0",
    "reactabular-table": "8.14.0",
    "segmentize": "0.4.1",
    "universal-router": "9.0.1",
    "validate.js": "0.13.1"
  },
  "devDependencies": {
    "bluebird": "3.7.2",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chance": "1.1.7",
    "chromedriver": "88.0.0",
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "7.1.1",
    "copy-webpack-plugin": "7.0.0",
    "css-loader": "5.0.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "4.5.1",
    "http-server": "0.12.3",
    "imports-loader": "1.2.0",
    "jscpd": "3.3.23",
    "json-loader": "0.5.7",
    "karma": "5.2.3",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-junit-reporter": "2.0.1",
    "karma-mocha": "2.0.1",
    "karma-mocha-reporter": "2.2.5",
    "karma-sinon": "1.0.5",
    "karma-sourcemap-loader": "0.3.8",
    "karma-webpack": "5.0.0-alpha.5",
    "lodash-webpack-plugin": "0.11.6",
    "mini-css-extract-plugin": "1.3.3",
    "mocha": "8.2.1",
    "nightwatch": "1.5.1",
    "puppeteer": "5.5.0",
    "sinon": "9.2.3",
    "style-loader": "2.0.0",
    "ts-loader": "8.0.14",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-loader": "3.5.4",
    "typescript": "4.1.3",
    "url-loader": "4.1.1",
    "webpack": "5.12.1",
    "webpack-bundle-analyzer": "4.3.0",
    "webpack-bundle-size-analyzer": "3.1.0",
    "webpack-cli": "4.3.1",
    "webpack-dev-server": "3.11.1",
    "webpack-hud": "0.1.2"
  },
  "engines": {
    "node": ">10.x",
    "npm": ">5.x"
  }
}
